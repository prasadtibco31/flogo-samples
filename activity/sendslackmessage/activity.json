    {
      "name": "Webhook",
      "type": "string",
      "required": true
    },
    {
      "name": "Channel",
      "type": "string",
      "required": false
    },
    {
      "name": "Message",
      "type": "string",
      "required": true
    },
    {
      "name": "Username",
      "type": "string",
      "required": false
    },
    {
      "name": "Iconemoji",
      "type": "string",
      "required": false
    }
  ],
  "outputs": [
    {
      "name": "result",
      "type": "string"
    }
  ]
